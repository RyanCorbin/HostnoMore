<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="HostnoMore.Views.MenuTwoContainerPage"
    xmlns:views="clr-namespace:HostnoMore.Views">
    <TabbedPage.ToolbarItems>
        <ToolbarItem Icon="shoppingcart.png" Command="{Binding GoToCart}"/>
    </TabbedPage.ToolbarItems>
    <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
        <Image Source="Chick.jpg"/>
        <Button Text="Combos" Command="{Binding NavigateCommand}"/>
        <Button Text="Entrees" Command="{Binding NavigateCommand2}"/>
        <Button Text="SideAndDrinks" Command="{Binding NavigateCommand3}"/>
    </StackLayout>
</TabbedPage>